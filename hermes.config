[global]
strategy = 'all'
filter = false
log_level = 'trace'
clear_packets_interval = 100

[telemetry]
enabled = false
host = '127.0.0.1'
port = 3001

[[chains]]                                                               
id = 'agoricstage-15'
rpc_addr = 'http://143.198.36.204:26657'
grpc_addr = 'http://143.198.36.204:9090'
websocket_addr = 'ws://143.198.36.204:26657/websocket'
rpc_timeout = '10s'
account_prefix = 'agoric'
key_name = 'stagekey'
store_prefix = 'ibc'
max_gas = 3000000
gas_price = { price = 0.001, denom = 'urun' }
gas_adjustment = 0.1
clock_drift = '5s'
